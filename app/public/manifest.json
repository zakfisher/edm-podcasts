{
  "name": "Westfield Wayfinding Prototype",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.15",
  "manifest_version": 2,
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ],
      "persistant": true
    }
  },
  "bluetooth": {},
  "permissions": [
    "http://*/",
    "https://*/",
    "webview",
    "desktopCapture",
    "storage",
    "unlimitedStorage",
    "power",
    "fullscreen"
  ],
  "kiosk_enabled": true
}